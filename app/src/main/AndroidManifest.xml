<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.asociacion.ferias">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".patrocinadores.Lista_Patrocinadores"
            android:label="@string/title_activity_lista__patrocinadores"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".cultura.Lista_Cultura"
            android:label="@string/title_activity_lista__cultura"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".conciertos.Lista_Conciertos"
            android:label="@string/title_activity_lista__conciertos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>